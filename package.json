{
  "name": "bullmq-exporter",
  "version": "1.0.0",
  "description": "Service that can be used to monitor BullMQ by providing Prometheus metrics and a Bullmq dashboard secured behind a login wall.",
  "main": "src/server.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 0",
    "lint": "eslint . --ext ts",
    "prettier": "prettier -w src/**/*.ts",
    "build": "tsc",
    "clean": "rm -rf node_modules dist",
    "run": "node dist/server.js",
    "dev": "ts-node src/server.ts",
    "ts-node": "ts-node src/server.ts",
    "nodemon": "nodemon"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@bull-board/api": "^4.12.2",
    "@bull-board/express": "^4.12.2",
    "bullmq": "^3.15.8",
    "connect-ensure-login": "^0.1.1",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "express-winston": "^4.2.0",
    "ioredis": "^5.4.1",
    "parse-duration": "^1.1.0",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "prom-client": "^14.2.0",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@types/connect-ensure-login": "^0.1.9",
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/passport": "^1.0.16",
    "@types/passport-local": "^1.0.38",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.0",
    "husky": "^8.0.3",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "npx ts-node src/server.ts"
  }
}
